{
  "name": "try-fetch",
  "version": "0.0.4",
  "description": "js fetch wrapper",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "./dist/index.mjs",
  "type": "module",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "format": "dprint fmt",
    "collect": "cat ./src/* | grep -vE 'import|dprint' > index.ts",
    "minify": "uglifyjs --mangle --compress -o ./dist/index.js -- ./dist/index.js",
    "build": "npm run collect; npm run format && tsc; rm index.ts; npm run minify",
    "test": "jest --verbose ./tests",
    "test-watch": "jest --watch --verbose ./tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DedaVnuk/try-fetch.git"
  },
  "keywords": [
    "fetch",
    "typescript",
    "ts",
    "utils",
    "try/catch",
    "types",
    "abort signal",
    "refetch"
  ],
  "author": "Sumarokov Vladimir <sumarokov.world@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DedaVnuk/try-fetch/issues"
  },
  "homepage": "https://github.com/DedaVnuk/try-fetch#readme",
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "dprint": "^0.44.0",
    "jest": "^29.6.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "uglify-js": "^3.17.4"
  }
}
